{
  "name": "@workspace/three-js-200",
  "version": "1.0.0",
  "dependencies": {
    "parcel": "latest"
  },
  "scripts": {
    "clean": "rm -rf dist .parcel-cache",
    "build": "env max-old-space-size=4000 node ./node_modules/.bin/parcel build --no-optimize --no-cache",
    "build:optimize": "env max-old-space-size=4000 node ./node_modules/.bin/parcel build --no-cache"
  },
  "targets": {
    "default": {
      "source": "./src/index.js",
      "distDir": "./dist",
      "sourceMap": false
    }
  }
}
